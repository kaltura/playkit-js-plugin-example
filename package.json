    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "css-loader": "^6.7.1",
    "eslint": "^8.56.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-mocha": "^10.1.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.0.0",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "sinon": "^14.0.0",
