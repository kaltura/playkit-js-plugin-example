    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "@playkit-js/kaltura-player-js": "canary",
    "@playkit-js/playkit-js": "canary",
    "@playkit-js/playkit-js-ui": "canary",
    "preact": "10.19.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Jonathan T. Gold",
